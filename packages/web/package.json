{
  "name": "@federated-graphql-cicd/web",
  "version": "3.0.4",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "next": "^9.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "jest": "^26.2.2"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format:check": "prettier --check \"**/*.{js,jsx,yml,yaml,json,css,scss,md}\"",
    "format": "prettier --write \"**/*.{js,jsx,yml,yaml,json,css,scss,md}\"",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "docker:login": "echo \"$DOCKERHUB_PASSWORD\" | docker login \"$DOCKER_REGISTRY_URL\" --username \"$DOCKERHUB_USERNAME\" --password-stdin",
    "docker:build": "docker build -t sincronize/federated-graphql-cicid-api-web:latest .",
    "docker:tag-version": "docker tag sincronize/federated-graphql-cicid-api-web:latest sincronize/federated-graphql-cicid-api-web:$LATEST_TAG",
    "docker:push": "docker push sincronize/federated-graphql-cicid-api-web:latest",
    "ci:test": "cross-env NODE_ENV=test jest --ci --runInBand --detectOpenHandles --forceExit",
    "ci:docker": "npm run docker:build && docker:tag-version && npm run docker:push",
    "ci:deploy": "npm run ci:docker"
  }
}
